{
  "name": "Onyx",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#b66"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#696"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "storage.modifier",
        "storage.type",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#777"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#ac6dac"
      }
    },
    {
      "name": "Global definitions",
      "scope": [
        "entity.name",
        "entity.name.function",
        "entity.name.method",
      ],
      "settings": {
        "foreground": "#49b"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
      ],
      "settings": {
        "foreground": "#d18c8c",
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#3f3f3f"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#660000"
      }
    },
  ],
  "colors": {
    "editor.background": "#222",
    "editor.foreground": "#ccc",
    "editor.lineHighlightBackground": "#101010",
    "editor.selectionBackground": "#003863",
    "editor.selectionHighlightBackground": "#444",
    "panel.background": "#101010",
    "badge.background": "#0c0e2e",
    "sideBar.background": "#101010",
    "editorGroupHeader.tabsBackground": "#101010",
    "activityBar.background": "#101010",
    "activityBar.foreground": "#ccc",
    "editorLineNumber.foreground": "#aaa",
    "editorCursor.foreground": "#003863",
    "editorCursor.background": "#ccc",
    "editor.findMatchBackground": "#942503",
    "editor.findMatchHighlightBackground": "#521300",
    "gitDecoration.addedResourceForeground": "#696",
    "gitDecoration.modifiedResourceForeground": "#49b",
    "gitDecoration.untrackedResourceForeground": "#3f3f3f",
    "gitDecoration.deletedResourceForeground": "#d66",
    "gitDecoration.conflictingResourceForeground": "#660000",
    "gitDecoration.ignoredResourceForeground": "#3f3f3f",
    "statusBar.background": "#0c0e2e",
    "statusBar.foreground": "#ccc",
    "statusBar.debuggingBackground": "#bf360c",
    "statusBar.debuggingForeground": "#ccc",
    "statusBar.noFolderBackground": "#4a148c",
    "statusBar.noFolderForeground": "#ccc",
    "terminal.ansiWhite": "#ccc",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#325CC0",
    "terminal.ansiCyan": "#0083B2",
    "terminal.ansiGreen": "#448C27",
    "terminal.ansiMagenta": "#7A3E9D",
    "terminal.ansiRed": "#AA3731",
    "terminal.ansiYellow": "#CB9000",
    "terminal.ansiBrightWhite": "#ccc",
    "terminal.ansiBrightBlack": "#777777",
    "terminal.ansiBrightBlue": "#007ACC",
    "terminal.ansiBrightCyan": "#00AACB",
    "terminal.ansiBrightGreen": "#60CB00",
    "terminal.ansiBrightMagenta": "#E64CE6",
    "terminal.ansiBrightRed": "#F05050",
    "terminal.ansiBrightYellow": "#FFBC5D"
  },
}